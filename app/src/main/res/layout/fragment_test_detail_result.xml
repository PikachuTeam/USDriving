<?xml version="1.0" encoding="utf-8"?>
<tatteam.com.app_common.ui.drawable.FractionFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/main_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tvType"
            android:layout_width="match_parent"
            android:layout_height="@dimen/common_size_3"
            android:background="@color/written_test_color_2"
            android:gravity="center"
            android:text="Correct Answers"
            android:textColor="@color/white"
            android:textSize="@dimen/common_text_size_10"
            android:textStyle="bold" />

        <ListView
            android:id="@+id/dmvWrittenTestList"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@color/white"
            android:dividerHeight="0dp"></ListView>

    </LinearLayout>
</tatteam.com.app_common.ui.drawable.FractionFrameLayout>
