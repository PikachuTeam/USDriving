    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerVertical="true"
    android:layout_gravity="center"
    android:gravity="center_horizontal"
    android:paddingLeft="@dimen/common_size_30"
    android:paddingRight="@dimen/common_size_30"
    android:paddingTop="@dimen/common_size_10"
    android:paddingBottom="@dimen/common_size_10"
    android:background="@color/window_background"
    >

    <ListView
        android:id="@+id/Chooselist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:divider="@null"
        android:dividerHeight="0dp"
        android:scrollbarStyle="outsideInset"
        android:padding="@dimen/common_size_5"
        ></ListView>

</tatteam.com.app_common.ui.drawable.FractionFrameLayout>
